<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:bind="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/root_include_service_home"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include
            android:id="@+id/include_diagnostic"
            layout="@layout/include_card_service"
            android:layout_width="135dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            bind:image="@{@drawable/ic_chat}"
            bind:text='@{"AAAAAAsssss"}' />

        <include
            android:id="@+id/include_consultation"
            layout="@layout/include_card_service"
            android:layout_width="136dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline2"
            app:layout_constraintTop_toTopOf="parent"
            bind:image="@{@drawable/ic_chat}"
            bind:text='@{"AAAAAAsssss"}'
            tools:layout_editor_absoluteX="137dp" />

        <include
            android:id="@+id/include_nurse"
            layout="@layout/include_card_service"
            android:layout_width="136dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            bind:image="@{@drawable/ic_chat}"
            bind:text='@{"AAAAAAsssss"}' />

        <include
            android:id="@+id/include_ambulance"
            layout="@layout/include_card_service"
            android:layout_width="135dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guideline2"
            bind:image="@{@drawable/ic_chat}"
            bind:text='@{"AAAAAAsssss"}' />

        <include
            android:id="@+id/include_card_work"
            layout="@layout/include_card_service"
            android:layout_width="136dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guideline2"
            bind:image="@{@drawable/ic_chat}"
            bind:text='@{"AAAAAAsssss"}'
            tools:layout_editor_absoluteX="137dp" />

        <include
            android:id="@+id/include_medicinek"
            layout="@layout/include_card_service"
            android:layout_width="136dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guideline2"
            bind:image="@{@drawable/ic_chat}"
            bind:text='@{"AAAAAAsssss"}' />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintGuide_percent="0.5"
            android:orientation="horizontal"/>



    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>